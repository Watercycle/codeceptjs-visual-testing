{
  "name": "codeceptjs-visual-testing",
  "version": "1.0.0",
  "description": "A CodeceptJS E2E test helper that makes use of pixelmatch and a little dom interaction to make sure your UI doesn't unexpectedly change.",
  "main": "index.js",
  "scripts": {
    "test": "codeceptjs run --config tests/src/codecept.conf.js",
    "test:docker": "docker compose run codeceptjs",
    "test:build": "docker compose build"
  },
  "keywords": [
    "Codeceptjs",
    "Visual",
    "Testing",
    "UI",
    "Testing",
    "E2E",
    "Acceptance",
    "Testing",
    "Helper"
  ],
  "author": "Matthew Spero",
  "license": "MIT",
  "dependencies": {
    "pixelmatch": "^5.2.1",
    "pngjs": "^6.0.0"
  },
  "devDependencies": {
    "codeceptjs": "^3.2.2",
    "puppeteer": "^11.0.0"
  }
}
